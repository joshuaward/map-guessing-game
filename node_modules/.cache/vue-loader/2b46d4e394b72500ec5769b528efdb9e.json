 }}]\n\t\t\t.game__title {{ title }}\n\t\t\tdiv Incorrect: #[span {{ answersIncorrect }}]\n\t\t\n\t\ttransition-group(name=\"section\" tag=\"main\")\n\t\t\t//- start screen w/ play button\n\t\t\tsection.game__panel.game__panel--start(v-if=\"start\", key=\"1\")\n\t\t\t\t.game__panel-inner\n\t\t\t\t\th1.game__panel-title {{ title }}\n\t\t\t\t\tp.game__panel-intro Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat.\n\t\t\t\t\tbutton.game__panel-button(@click=\"getFlag\" type=\"button\") Start\n\t\t\t\t\t\n\t\t\t\t\t\n\t\t\t//- guessing screen w/ flag, input, and submit button\n\t\t\tsection.game__panel.game__panel--game(v-else-if=\"game\", key=\"2\")\n\t\t\t\t.game__panel-inner\n\t\t\t\t\theader.game__panel-header\n\t\t\t\t\t\th2.game__panel-question What Country's Flag is this?\n\t\t\t\t\t.game__panel-flag\n\t\t\t\t\t\timg(:src=\"gameCorrect.flag\")\n\t\t\t\t\t.game__panel-form\n\t\t\t\t\t\t.game__panel-answer(v-for=\"(item,i) in gameFlags\" :key=\"i\")\n\t\t\t\t\t\t\tinput(type=\"radio\" :id=\"[`answer--${i}`]\" :value=\"item.name\" name=\"answers\")\n\t\t\t\t\t\t\tlabel(v-bind:for=\"[`answer--${i}`]\") {{ item.name }}\n\t\t\t\t\t.game__panel-footer\n\t\t\t\t\t\tbutton.game__panel-submit(@click.prevent=\"checkAnswer\" type=\"button\") Check Answer\n\n\t\t\t\t.game__panel-error(v-if=\"error\") Please make a selection!\n\n\n\t\t\t//- correct answer panel\n\t\t\tsection.game__panel.game__panel--result(v-else-if=\"correctAnswer\", key=\"3\")\n\t\t\t\t.game__panel-inner\n\t\t\t\t\theader.game__panel-header\n\t\t\t\t\t\th2.game__panel-question.correct #[strong #[span {{ gameCorrect.name }}]] is CORRECT!\n\t\t\t\t\t.game__panel-correct\n\t\t\t\t\t\timg(:src=\"gameCorrect.flag\")\n\t\t\t\t\tbutton.game__panel-button(@click=\"getFlag\" type=\"button\") Next Flag\n\n\n\t\t\t//- incorrect answer panel\n\t\t\tsection.game__panel.game__panel--result(v-else-if=\"incorrectAnswer\", key=\"4\")\n\t\t\t\t.game__panel-inner\n\t\t\t\t\theader.game__panel-header\n\t\t\t\t\t\th2.game__panel-question.incorrect Inorrect! The correct answer is #[span {{ gameCorrect.name }}]\n\t\t\t\t\t.game__panel-correct\n\t\t\t\t\t\timg(:src=\"gameCorrect.flag\")\n\t\t\t\t\tbutton.game__panel-button(@click=\"getFlag\" type=\"button\") Next Flag\n\n\n\t\t\t//- results screen w/ next flag button\n\t\t\tsection.game__panel(v-else, key=\"5\")\n\t\t\t\t.game__panel-inner\n\t\t\t\t\tp END!\n\n\n\t//- \n\t</div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n\tname: 'App',\n\tdata() {\n\t\treturn {\n\t\t\ttitle: 'Flag Guessing Game',\n\t\t\tflags: [],\n\t\t\tstart: true,\n\t\t\terror: false,\n\t\t\tgame: false,\n\t\t\tgameFlags: [],\n\t\t\tgameCorrect: null,\n\t\t\tcorrectAnswer: false,\n\t\t\tincorrectAnswer: false,\n\t\t\tanswersCorrect: 0,\n\t\t\tanswersIncorrect: 0\n\t\t}\n\t},\n\tmethods: {\n\t\tgetData() {\n\t\t\taxios.get('https://restcountries.eu/rest/v2/all?fields=name;flag')\n\t\t\t\t.then(res => {\n\t\t\t\t\tthis.flags = res.data;\n\t\t\t\t\tconsole.log('Flags: ', this.flags)\n\t\t\t\t})\n\t\t},\n\t\tshuffleAnswers(array) {\n\t\t\tvar currentIndex = array.length;\n\t\t\tvar temporaryValue, randomIndex;\n\t\t\t// While there remain elements to shuffle...\n\t\t\twhile (0 !== currentIndex) {\n\t\t\t\t// Pick a remaining element...\n\t\t\t\trandomIndex = Math.floor(Math.random() * currentIndex);\n\t\t\t\tcurrentIndex -= 1;\n\t\t\t\t// And swap it with the current element.\n\t\t\t\ttemporaryValue = array[currentIndex];\n\t\t\t\tarray[currentIndex] = array[randomIndex];\n\t\t\t\tarray[randomIndex] = temporaryValue;\n\t\t\t}\n\t\t\treturn array;\t\t\t\n\t\t},\n\t\tgetFlag() {\n\t\t\tlet correct = Math.floor(Math.random() * this.gameFlags.length) + 1;\t\n\t\t\t\n\t\t\tfor(let i = 0; i < 4; i++) {\n\t\t\t\tlet answers = Math.floor(Math.random() * this.flags.length) + 1;\n\t\t\t\tthis.gameFlags.push(this.flags[answers]);\n\t\t\t}\n\n\t\t\tlet index = this.gameFlags.indexOf('undefined');\n\t\t\tconsole.log(index)\n\t\t\tif(index > -1) {\n\t\t\t\tthis.gameFlags.splice(index, 1);\n\t\t\t\tconsole.log('spliced!');\n\t\t\t} else {\n\t\t\t\tconsole.log('Cool beans!')\n\t\t\t}\n\n\t\t\tif(this.flags < this.gameFlags) {\n\t\t\t\tthis.game = false;\n\t\t\t}\n\t\t\tthis.correctAnswer = false;\n\t\t\tthis.incorrectAnswer = false;\n\t\t\tthis.start = false;\n\t\t\tthis.game = true;\n\t\t\tthis.gameCorrect = this.gameFlags[correct];\n\t\t\tthis.shuffleAnswers(this.gameFlags);\n\n\t\t\tconsole.log(this.flags)\n\t\t},\n\t\tcheckAnswer() {\n\t\t\tconst userGuess = this.$el.querySelector('.game__panel-answer input:checked');\n\t\t\tconst userGuessVal = userGuess.value;\n\t\t\tif (this.gameCorrect.name.localeCompare(userGuessVal) == 0) {\n\t\t\t\tthis.correctAnswer = true;\n\t\t\t\tthis.game = false;\n\t\t\t\tthis.gameFlags = [];\n\t\t\t\tthis.answersCorrect += 1;\n\t\t\t\tconst index = this.flags.indexOf(this.gameCorrect)\n\t\t\t\tthis.flags.splice(index, 1);\n\t\t\t} else {\n\t\t\t\tthis.incorrectAnswer = true;\n\t\t\t\tthis.game = false;\n\t\t\t\tthis.gameFlags = [];\n\t\t\t\tthis.answersIncorrect += 1;\n\t\t\t}\n\t\t}\n\t},\t\t\t\n\tcreated() {\n\t\tthis.getData();\n\t\tconsole.log(this.flags);\n\t}\n}\n\n// [\n// \t{\n// \t\t\"flag\": \"https://restcountries.eu/data/afg.svg\",\n// \t\t\"name\": \"Afghanistan\"\n// \t},\n// \t{\n// \t\t\"flag\": \"https://restcountries.eu/data/ala.svg\",\n// \t\t\"name\": \"Ã…land Islands\"\n// \t},\n// \t{\n// \t\t\"flag\": \"https://restcountries.eu/data/alb.svg\",\n// \t\t\"name\": \"Albania\"\n// \t},\n// \t{\n// \t\t\"flag\": \"https://restcountries.eu/data/dza.svg\",\n// \t\t\"name\": \"Algeria\"\n// \t},\n// \t{\n// \t\t\"flag\": \"https://restcountries.eu/data/asm.svg\",\n// \t\t\"name\": \"American Samoa\"\n// \t},\n// \t{\n// \t\t\"flag\": \"https://restcountries.eu/data/and.svg\",\n// \t\t\"name\": \